<html>
<body>
<canvas id="hello" height="600" width="700" style="border:solid 1px;">
</canvas>
<script>


var a=document.getElementById("hello");
var b=a.getContext('2d');


b.beginPath();
b.lineWidth="2";
b.strokeStyle="black";
b.rect(40,20,600,500);
b.fillStyle="red";
b.stroke();
b.fill();


b.beginPath();
b.lineWidth="2";
b.stokeStyle="black";
b.rect(140,70,400,400);
b.fillStyle="yellow";
b.stroke();
b.fill();



b.beginPath();
b.lineWidth="2";
b.stokeStyle="black";
b.rect(250,140,200,250);
b.fillStyle="green";
b.stroke();
b.fill();



b.beginPath();
b.lineWidth="2";
b.stokeStyle="black";
b.rect(300,240,100,20);
b.fillStyle="grey";
b.stroke();
b.fill();

document.write("<br><br><br<br><hr><br><br>");
</script>
</body>
</html>


